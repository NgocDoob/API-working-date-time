
{
    "manifest_version": 2,
    "name": "Working Date",
	"version": "0.0.2",
    "browser_action": {
      "default_icon": "images/logo.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "background",
		"https://ajax.googleapis.com/ajax/libs/angularjs/1.7.8/angular.min.js"
    ],
    "content_security_policy": "script-src https://ajax.googleapis.com/ajax/libs/angularjs/1.7.8/angular.min.js 'self' 'unsafe-eval'; object-src 'self'"
}